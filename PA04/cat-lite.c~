#include <stdio.h> 
#include <stdlib.h>
#include <string.h> 

int main(int argc, char * * argv) 
{
  
  FILE * fin1; 
   char c = ' '; 
 int i;
 for(i = 1; i < argc; i++)
 { 
   
   fin1 = fopen(argv[i], "r") ; 
   if(fin1 == NULL) 
   {
     fin1 = argv[i];
     while(c != EOF)
     { 
        c = fgetc(fin1);
        fputc(c);
   }
   while(c != EOF)
   {
     printf("%c",c); 
   }
   fclose(fin1);
 }
 return 0;
}
